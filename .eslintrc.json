{

  "extends": [
    "airbnb",
    "prettier"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "eslint-plugin-prettier",
    "jest",
    "react",
    "react-hooks"
  ],
  "rules": {
    "prettier/prettier": ["error"],
    "comma-dangle": ["error", "always-multiline"],
    "no-unused-vars": [2],
    "import/prefer-default-export": 0,
    "react-hooks/rules-of-hooks": "error",
    "react/no-danger": 2
  }
}
